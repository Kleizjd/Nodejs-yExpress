{
    "env": {
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "arrow-parents": "off",
        "comma-dangle": "off",
        "spaced-comment": "off",
        "object-curly-newline": "off",

        // "no-console": "off"
        // "linebreak-style": 0,
        // "global-require": 0,
        // "eslint linebreak-style": [0, "error", "windows"],
    }
}